{
    "name": "imageengine/image-cdn-wordpress",
    "description": "WordPress Image CDN plugin for ImageEngine",
    "type": "library",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Steve Kamerman",
            "email": "stevekamerman@gmail.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7",
        "phpcompatibility/phpcompatibility-wp": "*",
        "wp-coding-standards/wpcs": "*",
        "php-stubs/wordpress-stubs": "^5.6"
    },
    "repositories": [
        {
            "type": "vcs",
            "name": "imageengine/php-sdk",
            "url": "https://github.com/imgeng/demo-ie-php-sdk.git"
        }
    ],
    "require": {
        "imageengine/php-sdk": "^1.0",
        "php-http/guzzle7-adapter": "^1.0",
        "nyholm/psr7": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "ImageEngine\\PhpSdk\\Config\\": "config/"
        }
    }
}
